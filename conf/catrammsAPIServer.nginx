			rewrite ^/catramms/([0-9]+.[0-9]+.[0-9]+)/ingestionJob/([0-9]+)/switchToNewEncoder/?$ /api?x-api-version=$1&x-api-method=ingestionJobSwitchToEncoder&ingestionJobKey=$2 last;
