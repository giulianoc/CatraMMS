			rewrite ^/catramms/([0-9]+.[0-9]+.[0-9]+)/mediaItem/([0-9]+)/([0-9]+)/?$ /api?x-api-version=$1&x-api-method=updatePhysicalPath&mediaItemKey=$2&physicalPathKey=$3 last;
