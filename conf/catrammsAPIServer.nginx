			rewrite ^/catramms/([0-9]+.[0-9]+.[0-9]+)/ingestionJob/([0-9]+)/switchToNewEncoder/?$ /api?version=$1&method=ingestionJobSwitchToEncoder&ingestionJobKey=$2 last;
		}
		if ($request_method = PUT) {
