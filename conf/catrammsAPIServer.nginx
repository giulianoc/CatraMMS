			rewrite ^/catramms/([0-9]+.[0-9]+.[0-9]+)/workflowAsLibrary/([0-9]*)/?$ /api?x-api-version=$1&x-api-method=workflowAsLibraryContent&workflowLibraryKey=$2 last;
