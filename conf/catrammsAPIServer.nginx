			rewrite ^/catramms/([0-9]+.[0-9]+.[0-9]+)/encodingJob/?([0-9]*)/?$ /api?version=$1&method=encodingJobsStatus&encodingJobKey=$2 last;
		}
		if ($request_method = DELETE) {
