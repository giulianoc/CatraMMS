			rewrite ^/catramms/([0-9]+.[0-9]+.[0-9]+)/ingestionJob/liveProxy/overlayText/([0-9]*)/?$ /api?version=$1&method=changeLiveProxyOverlayText&ingestionJobKey=$2 last;
		}
		if ($request_method = PUT) {
